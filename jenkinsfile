pipeline{
    agent any
    environment {
        JAVA_HOME='C:/Program Files/Java/jdk-17.0.3.1'
    }
    stages{
        stage('Declarative: Checkout SCM') {
            steps {
                checkout scm
            }
        }
        stage('Clean Stage'){
            steps{
                dir("StockApp"){
                    bat 'mvn clean'
                }
            }
        }
                stage('Compile Stage'){
            steps{
                dir("StockApp"){
                    bat 'mvn compile'
                }
            }
        }
                stage('install Stage'){
            steps{
                dir("StockApp"){
                    bat 'mvn install'
                }
            }
        }
    }
}